{
  "name": "ap-repaire",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "setup": "npm install && npm run db:generate && npm run db:push && npm run db:seed",
    "docker:build": "docker build -t ap-repaire .",
    "docker:run": "docker run -p 3000:3000 ap-repaire",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:vercel": "vercel --prod",
    "deploy:railway": "railway up",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "storybook": "echo 'Storybook not available'",
    "storybook:build": "echo 'Storybook not available'",
    "analyze": "ANALYZE=true npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",

    "commit": "git-cz",
    "release": "standard-version",
    "docs:dev": "echo 'Documentation not available'",
    "docs:build": "echo 'Documentation not available'",
    "docs:export": "echo 'Documentation not available'",
    "clean": "rm -rf .next out dist",
    "clean:all": "rm -rf .next out dist node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "check-all": "npm run lint && npm run type-check && npm run test",

    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-table": "^8.13.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.10.16",
    "lucide-react": "^0.540.0",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^2.12.7",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.17",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.44.1",

    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bcryptjs": "^3.0.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^9.1.0",


    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",

    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",

    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5"
  },

  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
